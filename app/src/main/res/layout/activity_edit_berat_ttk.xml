<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    android:id="@+id/drawer_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:translationZ="100dp"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#eeeeee"
    android:clipChildren="false"
    >
    <com.wahana.wahanamobile.VerticalScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#eeeeee"
        android:clipChildren="false"
        xmlns:android="http://schemas.android.com/apk/res/android">
        <android.support.design.widget.CoordinatorLayout
            android:id="@+id/main_content"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"

            >

            <android.support.design.widget.AppBarLayout
                android:id="@+id/appbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

                <android.support.v7.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/toolbar_height"
                    android:background="@color/material_yellow"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    app:layout_scrollFlags="enterAlways"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>

            </android.support.design.widget.AppBarLayout>

            <RelativeLayout
                android:id="@+id/layout_form"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:translationZ="100dp">
                <RelativeLayout
                    android:id="@+id/info_layout"
                    android:layout_marginTop="@dimen/margin_toolbar"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:translationZ="100dp"
                    android:padding="15dp"
                    android:layout_marginLeft="@dimen/margin_root"
                    android:layout_marginRight="@dimen/margin_root"
                    android:layout_marginBottom="@dimen/margin_new"
                    android:orientation="vertical"
                    android:background="@drawable/rounded_layout"
                    >
                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        >
                        <TableRow>
                            <ImageView
                                android:id="@+id/calendar_label"
                                android:layout_margin="@dimen/margin_absensi_form"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:src="@drawable/calendar_icon"
                                android:textColor="@color/primary_dark"
                                />
                            <TextView
                                android:id="@+id/calendar_isi"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/margin_absensi_form"
                                android:textColor="@color/headermain_textcolor"
                                android:text="@string/calendar_isi"
                                android:textSize="16sp"/>
                        </TableRow>
                        <TableRow>
                            <ImageView
                                android:id="@+id/tanggal_jam_label"
                                android:layout_margin="@dimen/margin_absensi_form"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:src="@drawable/time_icon"
                                android:textColor="@color/primary_dark"/>
                            <TextView
                                android:id="@+id/tanggal_jam_isi"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/margin_absensi_form"
                                android:textColor="@color/headermain_textcolor"
                                android:text="@string/tanggal_jam_isi"
                                android:textSize="16sp"/>
                        </TableRow>

                    </TableLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        >
                        <TableLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TableRow>

                                <ImageView
                                    android:id="@+id/kurir_label"
                                    android:layout_margin="@dimen/margin_absensi_form"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/people_icon"
                                    android:textColor="@color/primary_dark"/>
                                <TextView
                                    android:id="@+id/pengisi_isi"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="@dimen/margin_absensi_form"
                                    android:textColor="@color/headermain_textcolor"
                                    android:text="@string/pengisi_absensi_isi"
                                    android:textSize="16sp"/>
                            </TableRow>
                        </TableLayout>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/main_info"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_below="@+id/info_layout"
                    android:translationZ="100dp"
                    android:padding="15dp"
                    android:layout_marginLeft="@dimen/margin_root"
                    android:layout_marginRight="@dimen/margin_root"
                    android:layout_marginBottom="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/rounded_layout"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TableLayout
                            android:id="@+id/table_terkirim1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TableRow>
                                <ImageView
                                    android:id="@+id/img_terkirim1"
                                    android:layout_margin="@dimen/margin_absensi_form"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ceklis_icon"
                                    android:textColor="@color/primary_dark"
                                    />
                                <TextView
                                    android:id="@+id/judul_status_terkirim_isi1"
                                    android:padding="5dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="@dimen/margin_absensi_form"
                                    android:textColor="@color/headermain_textcolor"
                                    android:text="INFO TTK"
                                    android:textSize="16sp"/>
                            </TableRow>


                        </TableLayout>


                        <TableLayout
                            android:layout_marginLeft="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TableRow
                                android:weightSum="1"
                                android:layout_width="wrap_content"
                                >

                                <TextView
                                    android:text="Nomor TTK"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:layout_width="wrap_content"
                                    android:textSize="15dp"
                                    />
                                <TextView
                                    android:text=":"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:layout_width="wrap_content"
                                    android:textSize="15dp"
                                    />

                                <TextView
                                    android:id="@+id/nottkinfo"
                                    android:text="ABC12345"
                                    android:textColor="@color/black"
                                    android:textSize="15dp"
                                    android:layout_width="0dp"
                                    android:layout_weight="0.8"/>

                            </TableRow>

                            <TableRow
                                android:weightSum="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                >

                                <TextView
                                    android:text="Pengirim"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:textSize="15dp"
                                    />
                                <TextView
                                    android:text=":"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:textSize="15dp"
                                    />

                                <TextView
                                    android:id="@+id/pengiriminfo"
                                    android:text="Donny Kusdianto"
                                    android:textColor="@color/black"
                                    android:layout_width="0dp"
                                    android:textSize="15dp"
                                    android:layout_weight="0.8"/>

                            </TableRow>

                            <TableRow
                                android:weightSum="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                >

                                <TextView
                                    android:text="Berat"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:textSize="15dp"
                                    />
                                <TextView
                                    android:text=":"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:textSize="15dp"
                                    />

                                <TextView
                                    android:id="@+id/beratinfo"
                                    android:text="2 Kg"
                                    android:textColor="@color/black"
                                    android:textSize="15dp"
                                    android:layout_width="0dp"
                                    android:layout_weight="0.8"/>

                            </TableRow>

                            <TableRow
                                android:weightSum="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                >

                                <TextView
                                    android:text="Volume"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:textSize="15dp"
                                    />
                                <TextView
                                    android:text=":"
                                    android:textColor="@color/black"
                                    android:layout_weight="0.1"
                                    android:textSize="15dp"
                                    />

                                <TextView
                                    android:id="@+id/volumeinfo"
                                    android:text="1 x 1 x 1"
                                    android:textColor="@color/black"
                                    android:textSize="15dp"
                                    android:layout_width="0dp"
                                    android:layout_weight="0.8"/>

                            </TableRow>

                        </TableLayout>

                    </LinearLayout>






                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/main_layout"
                    android:layout_marginTop="10dp"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:translationZ="100dp"
                    android:padding="15dp"
                    android:layout_marginLeft="@dimen/margin_root"
                    android:layout_marginRight="@dimen/margin_root"
                    android:layout_marginBottom="@dimen/margin_root"
                    android:orientation="vertical"
                    android:background="@drawable/rounded_layout"
                    android:layout_below="@+id/main_info"
                    >
                    <TableLayout
                        android:id="@+id/table_terkirim"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <TableRow>
                            <ImageView
                                android:id="@+id/img_terkirim"
                                android:layout_margin="@dimen/margin_absensi_form"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:src="@drawable/ceklis_icon"

                                android:textColor="@color/primary_dark"
                                />
                            <TextView
                                android:id="@+id/judul_status_terkirim_isi"
                                android:padding="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/margin_absensi_form"
                                android:textColor="@color/headermain_textcolor"
                                android:text="EDIT BERAT"
                                android:textSize="16sp"/>
                        </TableRow>
                    </TableLayout>

                    <LinearLayout
                        android:id="@+id/tabel_penerima"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/layout_input_penerima"
                        android:orientation="horizontal"
                        android:weightSum="1.0"></LinearLayout>
                    <LinearLayout
                        android:id="@+id/layout_keterangan"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="1.0"
                        android:layout_below="@+id/table_terkirim">
                        <TextView
                            android:id="@+id/keterangan_label"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_absensi_form"
                            android:textColor="@color/headermain_textcolor"
                            android:text="BERAT AKTUAL"
                            android:textSize="16sp"
                            android:layout_weight="0.7"/>
                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/berat_aktual"
                        android:layout_below="@+id/layout_keterangan">
                        <EditText
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:id="@+id/berat"
                            android:background="@drawable/background_input"
                            android:hint="Masukkan berat aktual"
                            android:inputType="number"
                            android:textColor="@color/dark"
                            android:imeOptions="actionDone"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Kg"
                            android:layout_marginTop="14dp"
                            android:layout_marginRight="5dp"
                            android:textColor="#000000"
                            android:layout_alignParentRight="true"/>

                    </RelativeLayout>
                    <TableLayout
                        android:id="@+id/tabel_volumetrik"
                        android:layout_below="@+id/berat_aktual"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:stretchColumns="3"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp">
                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:weightSum="1">

                            <LinearLayout
                                android:layout_weight="0.3"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="5dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="PANJANG"
                                    android:textColor="@color/headermain_textcolor"
                                    android:textSize="16sp"
                                    />

                                <RelativeLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="match_parent">

                                    <EditText
                                        android:id="@+id/panjang"
                                        android:layout_width="match_parent"
                                        android:layout_height="40dp"
                                        android:layout_marginTop="5dp"
                                        android:inputType="numberDecimal"
                                        android:textColor="@color/dark"
                                        android:background="@drawable/background_input"
                                        android:maxLength="4"
                                        android:imeOptions="actionDone"/>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="cm"
                                        android:textSize="15sp"
                                        android:layout_marginTop="14dp"
                                        android:layout_marginRight="5dp"
                                        android:textColor="#000000"
                                        android:layout_alignParentRight="true"/>

                                </RelativeLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_weight="0.3"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="5dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="LEBAR"
                                    android:textColor="@color/headermain_textcolor"
                                    android:textSize="16sp"/>

                                <RelativeLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="match_parent">

                                    <EditText
                                        android:id="@+id/lebar"
                                        android:layout_width="match_parent"
                                        android:layout_height="40dp"
                                        android:layout_marginTop="5dp"
                                        android:textColor="@color/dark"
                                        android:inputType="numberDecimal"
                                        android:background="@drawable/background_input"
                                        android:maxLength="4"
                                        android:imeOptions="actionDone"/>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="cm"
                                        android:textSize="15sp"
                                        android:layout_marginTop="14dp"
                                        android:layout_marginRight="5dp"
                                        android:textColor="#000000"
                                        android:layout_alignParentRight="true"/>

                                </RelativeLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_weight="0.3"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="5dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="TINGGI"
                                    android:textColor="@color/headermain_textcolor"
                                    android:textSize="16sp"/>

                                <RelativeLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="match_parent">

                                    <EditText
                                        android:id="@+id/tinggi"
                                        android:layout_width="match_parent"
                                        android:layout_height="40dp"
                                        android:layout_marginTop="5dp"
                                        android:inputType="numberDecimal"
                                        android:textColor="@color/dark"
                                        android:background="@drawable/background_input"
                                        android:maxLength="4"
                                        />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="cm"
                                        android:textSize="15sp"
                                        android:layout_marginTop="14dp"
                                        android:layout_marginRight="5dp"
                                        android:textColor="#000000"
                                        android:layout_alignParentRight="true"/>

                                </RelativeLayout>
                            </LinearLayout>
                        </TableRow>
                    </TableLayout>
                    <LinearLayout
                        android:id="@+id/tabel_foto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tabel_volumetrik"
                        android:orientation="horizontal"
                        android:weightSum="1.0">
                        <TextView
                            android:id="@+id/foto_diri_label"
                            android:layout_margin="@dimen/margin_absensi_form"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="FOTO PAKET"
                            android:textColor="@color/main_textcolor"
                            android:layout_weight="0.7"
                            android:textSize="16sp"/>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/listView"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_below="@+id/tabel_foto"
                        />

                    <com.wahana.wahanamobile.helper.ButtonPlus
                        style="@style/button"
                        android:id="@+id/btn_submit_foto"
                        android:layout_width="90dp"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:layout_height="35dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:text="Ambil Foto"
                        android:background="@drawable/roundedbutton"
                        android:textColor="@color/white"
                        android:layout_below="@+id/listView"
                        android:layout_marginTop="10dp"
                        />

                    <LinearLayout
                        android:id="@+id/tabel_foto2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/btn_submit_foto"
                        android:orientation="horizontal"
                        android:weightSum="1.0">
                        <TextView
                            android:id="@+id/foto_diri_label2"
                            android:layout_margin="@dimen/margin_absensi_form"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="FOTO BERAT "
                            android:textColor="@color/main_textcolor"
                            android:layout_weight="0.7"
                            android:textSize="16sp"/>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/listView2"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_below="@+id/tabel_foto2"
                        />

                    <com.wahana.wahanamobile.helper.ButtonPlus
                        style="@style/button"
                        android:id="@+id/btn_submit_foto2"
                        android:layout_width="90dp"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:layout_height="35dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:text="Ambil Foto "
                        android:background="@drawable/roundedbutton"
                        android:textColor="@color/white"
                        android:layout_below="@+id/listView2"
                        android:layout_marginTop="10dp"
                        />

                    <LinearLayout
                        android:id="@+id/tabel_foto3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/btn_submit_foto2"
                        android:orientation="horizontal"
                        android:weightSum="1.0">
                        <TextView
                            android:id="@+id/foto_diri_label3"
                            android:layout_margin="@dimen/margin_absensi_form"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="FOTO PANJANG"
                            android:textColor="@color/main_textcolor"
                            android:layout_weight="0.7"
                            android:textSize="16sp"/>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/listView3"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_below="@+id/tabel_foto3"
                        />

                    <com.wahana.wahanamobile.helper.ButtonPlus
                        style="@style/button"
                        android:id="@+id/btn_submit_foto3"
                        android:layout_width="90dp"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:layout_height="35dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:text="Ambil Foto "
                        android:background="@drawable/roundedbutton"
                        android:textColor="@color/white"
                        android:layout_below="@+id/listView3"
                        android:layout_marginTop="10dp"
                        />

                    <LinearLayout
                        android:id="@+id/tabel_foto4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/btn_submit_foto3"
                        android:orientation="horizontal"
                        android:weightSum="1.0">
                        <TextView
                            android:id="@+id/foto_diri_label4"
                            android:layout_margin="@dimen/margin_absensi_form"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="FOTO LEBAR"
                            android:textColor="@color/main_textcolor"
                            android:layout_weight="0.7"
                            android:textSize="16sp"/>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/listView4"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_below="@+id/tabel_foto4"
                        />

                    <com.wahana.wahanamobile.helper.ButtonPlus
                        style="@style/button"
                        android:id="@+id/btn_submit_foto4"
                        android:layout_width="90dp"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:layout_height="35dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:text="Ambil Foto "
                        android:background="@drawable/roundedbutton"
                        android:textColor="@color/white"
                        android:layout_below="@+id/listView4"
                        android:layout_marginTop="10dp"
                        />

                    <LinearLayout
                        android:id="@+id/tabel_foto5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/btn_submit_foto4"
                        android:orientation="horizontal"
                        android:weightSum="1.0">
                        <TextView
                            android:id="@+id/foto_diri_label5"
                            android:layout_margin="@dimen/margin_absensi_form"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="FOTO TINGGI"
                            android:textColor="@color/main_textcolor"
                            android:layout_weight="0.7"
                            android:textSize="16sp"/>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/listView5"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_below="@+id/tabel_foto5"
                        />

                    <com.wahana.wahanamobile.helper.ButtonPlus
                        style="@style/button"
                        android:id="@+id/btn_submit_foto5"
                        android:layout_width="90dp"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:layout_height="35dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:text="Ambil Foto "
                        android:background="@drawable/roundedbutton"
                        android:textColor="@color/white"
                        android:layout_below="@+id/listView5"
