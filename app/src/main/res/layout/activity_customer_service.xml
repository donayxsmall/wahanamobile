<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    android:id="@+id/drawer_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:translationZ="100dp"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#eeeeee"
    android:clipChildren="false"
    >
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#eeeeee"
        android:clipChildren="true"
        xmlns:android="http://schemas.android.com/apk/res/android">
        <android.support.design.widget.CoordinatorLayout
            android:id="@+id/main_content"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"

            >

            <android.support.design.widget.AppBarLayout
                android:id="@+id/appbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

                <android.support.v7.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="160dp"
                    android:background="@color/material_yellow"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    app:layout_scrollFlags="enterAlways"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>

            </android.support.design.widget.AppBarLayout>
            <RelativeLayout
                android:id="@+id/layout_form"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:translationZ="100dp">
            <RelativeLayout
                android:id="@+id/info_layout"
                android:layout_marginTop="110dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:translationZ="100dp"
                android:padding="10dp"
                android:layout_marginLeft="@dimen/margin_new"
                android:layout_marginRight="@dimen/margin_new"
                android:layout_marginBottom="@dimen/margin_new"
                android:background="@drawable/rounded_layout"
                >
                <LinearLayout
                    android:id="@+id/alamat_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_weight="0.15"
                            android:src="@drawable/alamat_icon"/>
                        <TextView
                            android:id="@+id/alamat_label"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.85"
                            android:textSize="12dp"
                            android:paddingLeft="5dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center_vertical"
                            android:textColor="@color/main_textcolor"
                            android:text="@string/alamat_label"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_weight="0.15"
                            android:src="@drawable/phone_icon"/>
                        <TextView
                            android:id="@+id/phone_label"
                            android:layout_width="0dp"
                            android:paddingLeft="5dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.85"
                            android:textSize="12dp"
                            android:textColor="@color/main_textcolor"
                            android:text="@string/phone_label"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0"
                        >
                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_weight="0.15"
                            android:src="@drawable/fax_icon"/>
                        <TextView
                            android:id="@+id/fax_label"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.85"
                            android:paddingLeft="5dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp"
                            android:textColor="@color/main_textcolor"
                            android:text="@string/fax_label"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5.0">
                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="30dp"
                                android:layout_weight="0.2"
                                android:src="@drawable/facebook_icon"/>
                            <TextView
                                android:id="@+id/facebook_label"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.8"
                                android:paddingLeft="5dp"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:textSize="12dp"
                                android:textColor="@color/primary_darker"
                                android:text="@string/facebook_label"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5.0">
                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="30dp"
                                android:layout_weight="0.2"
                                android:src="@drawable/instagram_icon"/>
                            <TextView
                                android:id="@+id/instagram_label"
                                android:layout_width="0dp"
                                android:paddingLeft="5dp"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.8"
                                android:textSize="12dp"
                                android:textColor="@color/primary_darker"
                                android:text="@string/instagram_label"/>
                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/metro_layout"
                    android:paddingTop="5dp"
                    android:layout_below="@+id/alamat_layout"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.15"
                            android:src="@drawable/metro_icon"/>
                        <TextView
                            android:id="@+id/metro_label"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.85"
                            android:textSize="16dp"
                            android:padding="@dimen/margin_new"
                            android:textColor="@color/main_textcolor"
                            android:text="@string/metro_label"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="0.5"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <ImageView
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/lokasi_warna_icon"/>
                                <TextView
                                    android:id="@+id/wilayah_jakarta_label"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="5dp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:textSize="10dp"
                                    android:textColor="@color/main_textcolor"
                                    android:text="@string/wilayah_jakarta_label"/>
                            </LinearLayout>
                            <TableLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                <TableRow>
                                        <ImageView
                                            android:layout_width="20dp"
                                            android:layout_height="20dp"
                                            android:layout_marginLeft="4dp"
                                            android:src="@drawable/whatsapp_icon"/>

                                        <TextView
                                            android:id="@+id/wa_label"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:paddingLeft="5dp"
                                            android:gravity="center_vertical"
                                            android:layout_gravity="center_vertical"
                                            android:textSize="10dp"
                                            android:textColor="@color/main_textcolor"
                                            android:text="@string/wa_label"/>
                                    </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email_label"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label"/>
                                </TableRow>
                                </TableLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingTop="10dp">
                                <ImageView
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/lokasi_warna_icon"/>
                                <TextView
                                    android:id="@+id/wilayah_jawa_barat_label"
                                    android:layout_width="wrap_content"
                                    android:paddingTop="10dp"
                                    android:layout_height="wrap_content"
                                    android:textSize="10dp"
                                    android:textColor="@color/main_textcolor"
                                    android:text="@string/wilayah_jawa_barat_label"/>
                            </LinearLayout>
                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>
                                    <TextView
                                        android:id="@+id/wa_label_jawa_barat"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_jawa_barat"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email_label_jawa_barat"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label_jawa_barat"/>
                                </TableRow>
                            </TableLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="0.5"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <ImageView
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/lokasi_warna_icon"/>
                                <TextView
                                    android:id="@+id/wilayah_banten_label"
                                    android:layout_width="wrap_content"
                                    android:paddingLeft="5dp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:textSize="10dp"
                                    android:textColor="@color/main_textcolor"
                                    android:text="@string/wilayah_banten_label"/>
                            </LinearLayout>
                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>

                                    <TextView
                                        android:id="@+id/wa_label_banten"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_banten"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email_label_banten"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textSize="10dp"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label_banten"/>
                                </TableRow>
                            </TableLayout>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/non_metro_layout"
                    android:layout_below="@+id/metro_layout"
                    android:orientation="vertical"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.15"
                            android:src="@drawable/non_metro_icon"/>
                        <TextView
                            android:id="@+id/non_metro_label"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.85"
                            android:textSize="16dp"
                            android:padding="@dimen/margin_new"
                            android:textColor="@color/main_textcolor"
                            android:text="@string/non_metro_label"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="0.5"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                >
                                <ImageView

                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/lokasi_warna_icon"/>
                                <TextView
                                    android:id="@+id/wilayah_jawa_timur_label"
                                    android:layout_width="wrap_content"
                                    android:paddingLeft="5dp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:textSize="10dp"
                                    android:textColor="@color/main_textcolor"
                                    android:text="@string/wilayah_jawa_timur_label"/>
                            </LinearLayout>
                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>
                                    <TextView
                                        android:id="@+id/wa_label_jawa_timur"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_jawa_timur"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email_label_jawa_timur"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label_jawa_timur"/>
                                </TableRow>
                            </TableLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingTop="10dp"
                                android:layout_marginTop="14dp">
                                <ImageView
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/lokasi_warna_icon"/>
                                <TextView
                                    android:id="@+id/wilayah_jawa_tengah_label"
                                    android:layout_width="wrap_content"
                                    android:paddingLeft="5dp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:textSize="10dp"
                                    android:textColor="@color/main_textcolor"
                                    android:text="@string/wilayah_jawa_tengah_label"/>
                            </LinearLayout>
                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>
                                    <TextView
                                        android:id="@+id/wa_label_jawa_tengah"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_jawa_tengah"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email_label_jawa_tengah"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label_jawa_tengah"/>
                                </TableRow>
                            </TableLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="0.5"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <ImageView
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/lokasi_warna_icon"/>
                                <TextView
                                    android:id="@+id/wilayah_kalimantan_label"
                                    android:layout_width="wrap_content"
                                    android:paddingLeft="5dp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:textSize="10dp"
                                    android:textColor="@color/main_textcolor"
                                    android:text="@string/wilayah_kalimantan_label"/>
                            </LinearLayout>
                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>
                                    <TextView
                                        android:id="@+id/wa_label_kalimantan"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_kalimantan"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email_label_kalimantan"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label_kalimantan"/>
                                </TableRow>
                            </TableLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingTop="10dp">
                                <ImageView
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/lokasi_warna_icon"/>
                                <TextView
                                    android:id="@+id/wilayah_sumatera_label"
                                    android:layout_width="wrap_content"
                                    android:paddingLeft="5dp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:textSize="10dp"
                                    android:textColor="@color/main_textcolor"
                                    android:text="@string/wilayah_sumatera_label"/>
                            </LinearLayout>
                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>
                                    <TextView
                                        android:id="@+id/wa_label_sumatera"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_sumatera"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email_label_sumatera"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label_sumatera"/>
                                </TableRow>
                            </TableLayout>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/retur_layout"
                    android:layout_below="@+id/non_metro_layout"
                    android:orientation="vertical"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.15"
                            android:src="@drawable/retur_icon"/>
                        <TextView
                            android:id="@+id/retur_label"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.85"
                            android:textSize="16dp"
                            android:padding="@dimen/margin_new"
                            android:textColor="@color/main_textcolor"
                            android:text="@string/retur_label"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1.0">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="0.5"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>
                                    <TextView
                                        android:id="@+id/wa_label_retur"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_retur"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginLeft="4dp"
                                        android:src="@drawable/whatsapp_icon"/>
                                    <TextView
                                        android:id="@+id/wa_label_retur2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/wa_label_retur2"/>
                                </TableRow>
                                <TableRow>
                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:src="@drawable/email_icon"/>
                                    <TextView
                                        android:id="@+id/email1_label_retur"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingLeft="5dp"
                                        android:gravity="center_vertical"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="10dp"
                                        android:textColor="@color/main_textcolor"
                                        android:text="@string/email_label_retur_1"/>
                                </TableRow>
                            </TableLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>

            </RelativeLayout>
        </android.support.design.widget.CoordinatorLayout>
    </ScrollView>
    <ListView
        android:id="@+id/list_slidermenu"
        android:layout_width="240dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:choiceMode="singleChoice"
        app:headerLayout="@layout/nav_header"
        android:divider="#eeeeee"
        android:dividerHeight="1dp"
        android:listSelector="@drawable/list_selector"
        android:background="#142631"/>
</android.support.v4.widget.DrawerLayout>

